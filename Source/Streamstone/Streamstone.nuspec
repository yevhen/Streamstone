<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
  <metadata>
    <id>Streamstone</id>
    <version>$version$</version>
    <authors>Yevhen Bobrov</authors>
    <owners>Yevhen Bobrov</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <licenseUrl>http://github.com/yevhen/Streamstone/blob/master/LICENSE</licenseUrl>
    <projectUrl>http://github.com/yevhen/Streamstone</projectUrl>
    <iconUrl>https://github.com/yevhen/Streamstone/raw/master/Logo.png</iconUrl>
    <description>Streamstone is a lightweight library which implements a low-level mechanics of Event Store on top of Windows Azure Table Storage. Handles concurrency conflicts via optimistic locking. Supports custom event and stream metadata. Optimized for batch processing.</description>
    <tags>event store eventstore eventsourcing event-sourcing CQRS azure table storage stream</tags>
    <dependencies>
      <group targetFramework=".NETStandard2.0">
        <dependency id="NETStandard.Library" version="2.0.0" exclude="Build,Analyzers" />
        <dependency id="WindowsAzure.Storage" version="8.3.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.5">
        <dependency id="WindowsAzure.Storage" version="8.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
  </metadata>
  <files>
    <file src="netstandard2.0\Streamstone.dll" target="lib/netstandard2.0" />
    <file src="netstandard2.0\Streamstone.pdb" target="lib/netstandard2.0" />
    <file src="netstandard2.0\Streamstone.xml" target="lib/netstandard2.0" />
    <file src="net46\Streamstone.dll" target="lib/net46" />
    <file src="net46\Streamstone.pdb" target="lib/net46" />
    <file src="net46\Streamstone.xml" target="lib/net46" />
  </files>
</package>